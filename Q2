#include <stdio.h>
#include <math.h>
int main() 
{
    int n,N,s=0;
    scanf("%d",&n);
    N=n;
    while(n>0)
    {
        s=s+(pow((n%10),3));
        n=n/10;
    }
    if(N==s)
        printf("%d is an armstrong number.",N);
    else
        printf("%d is not an armstrong number.",N);
    return 0;
}

